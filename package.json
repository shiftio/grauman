{
  "name": "grauman",
  "version": "0.2.8",
  "description": "Framework-agnostic Web Media Player that supports M4A, MP4, and Encrypted M3U8 playback.",
  "main": "dist/grauman.js",
  "module": "src/Grauman.js",
  "author": "Shift.io",
  "scripts": {
    "prepublish": "npm run build-prod",
    "build-dev": "./node_modules/.bin/webpack --display-modules --display-chunks --env dev",
    "build-prod": "./node_modules/.bin/webpack --env prod",
    "build": "npm run build-prod",
    "docs": "./node_modules/.bin/documentation build src/MediaPlayer.js src/MediaFile.js src/consts.js -f html -o dist/docs -c src/docs/documentation.yml --sort-order alpha",
    "postbuild-dev": "npm run docs",
    "postbuild-prod": "npm run docs"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "dist/**/**"
  ],
  "devDependencies": {
    "VideoFrame": "git+https://github.com/shiftio/VideoFrame.git#v0.2.3",
    "babel-core": "6.21.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.18.0",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.0.1",
    "css-element-queries": "git+https://github.com/shiftio/css-element-queries#5b85255",
    "css-loader": "0.28.1",
    "cssnano": "3.10.0",
    "documentation": "4.0.0-rc.0",
    "eslint": "3.16.1",
    "eslint-loader": "1.7.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.1",
    "hls.js": "0.7.7",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "merge": "^1.2.0",
    "mithril": "1.1.1",
    "node-sass": "^4.3.0",
    "pdf.js": "git+https://github.com/shiftio/pdfjs-dist#188e4c3",
    "postcss-loader": "2.0.5",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "three": "0.85.0",
    "url-loader": "0.5.8",
    "url-toolkit": "1.0.10",
    "webpack": "3.3.0",
    "webworkify": "1.4.0"
  }
}
